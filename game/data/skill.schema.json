{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "version": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "wit_effectiveness": {
      "type": "number"
    },
    "conditions": {
      "type": "object",
      "properties": {
        "phase": {
          "type": "string",
          "enum": ["EARLY", "MID", "LATE", "FINAL", "FINISH"]
        }
      },
      "additionalProperties": false
    },
    "effects": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "properties": {
            "min": {
              "type": "number"
            },
            "max": {
              "type": "number"
            }
          },
          "required": ["min", "max"]
        }
      }
    }
  },
  "required": ["version", "name", "wit_effectiveness", "conditions", "effects"]
}
